grammar org.xtext.example.p4dsl.P4Dsl with org.eclipse.xtext.common.Terminals

generate p4Dsl "http://www.xtext.org/example/p4dsl/P4Dsl"

Model:
	command+=Command*;

Command:
	'p4' P4Command;

P4Command:
	FileBasedCommand;
	
FileBasedCommand:
	Add | Edit | Delete;

Add:
	'add' name=ID;

Edit:
	'edit' name=ID;

Delete:
	'delete' name=ID;
